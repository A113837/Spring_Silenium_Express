<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student" class="com.seleniumexpress.di.Student">
		<property name="studentName" value="Mike"/>
		<property name="id" value="21" />
	</bean>
	
	<bean id="ashish" class="com.seleniumexpress.di.Student">
		<property name="studentName" value="Ashish"/>
		<property name="id" value="23" />
	</bean>

</beans>