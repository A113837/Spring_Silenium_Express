<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student" class="com.seleniumexpress.di.Student">
		<constructor-arg name="studentName" value="Mike" type="java.lang.String"/>
		<constructor-arg name="id" value="21" type="int"/>
	</bean>
	
		<bean id="dom" class="com.seleniumexpress.di.Student">
		<constructor-arg name="id" value="23" />
	</bean>
	


</beans>